{
  "name": "buildd",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "bun --filter @buildd/web dev",
    "build": "bun run --filter @buildd/web build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "agent": "bun run --filter @buildd/agent start",
    "agent:build": "bun run --filter @buildd/agent build"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.4.0",
    "vitest": "^4.0.18"
  }
}
