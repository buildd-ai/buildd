{
  "bunVersion": "1.x",
  "buildCommand": "bun run --filter @buildd/web build",
  "installCommand": "bun install",
  "framework": "nextjs",
  "outputDirectory": "apps/web/.next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- apps/web/ packages/",
  "git": {
    "deploymentEnabled": {
      "dev": false
    }
  }
}
